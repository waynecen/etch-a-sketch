[32m+[m[32m//script for etch-a-sketch[m
[32m+[m[32mconst container = document.querySelector('#container');[m
[32m+[m
[32m+[m[32mconst content = document.createElement('div');[m
[32m+[m[32mcontent.classList.add('content');[m
[32m+[m[32mcontainer.appendChild(content);[m
[32m+[m
[32m+[m[32mconst row = document.createElement('div');[m
[32m+[m[32mrow.classList.add('row');[m
[32m+[m[32mcontent.appendChild(row);[m
[32m+[m
[32m+[m[32mconst box = document.createElement('div');[m
[32m+[m[32mbox.classList.add('box');[m
[32m+[m[32mbox.textContent = 'boo';[m
[32m+[m[32mrow.appendChild(box);[m
