[32m+[m[32m@charset "UTF-8";[m
[32m+[m[32m/* stylesheet for etch-a-sketch */[m
[32m+[m[32m* {[m
[32m+[m[32m  box-sizing: border-box;[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mhtml, body {[m
[32m+[m[32m  min-height: 100%;[m
[32m+[m[32m  background: #fff;[m
[32m+[m[32m  color: #000;[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m/* sets the body height of the browser, so that backgrounds and div heights work correctly. Color and background will almost certainly be altered; they are just placeholders */[m
[32m+[m[32mbody {[m
[32m+[m[32m  font-size: 1.4rem;[m
[32m+[m[32m  text-rendering: optimizeLegibility;[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m/* sets the base font size for the document; turns on kerning pairs and ligatures */[m
[32m+[m[32mbody, ul, ol, dl {[m
[32m+[m[32m  margin: 0;[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32marticle, aside, audio,[m
[32m+[m[32mfooter, header, nav, section, video {[m
[32m+[m[32m  display: block;[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m/* ensures that older browsers will display HTML5 elements correctly */[m
[32m+[m[32mh1 {[m
[32m+[m[32m  font-size: 1.4rem;[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m/* begins to set up font sizes relative to base rem ‚Äì h1 has been used as an example */[m
[32m+[m[32mp {[m
[32m+[m[32m  -ms-word-break: break-all;[m
[32m+[m[32m  word-break: break-all;[m
[32m+[m[32m  word-break: break-word;[m
[32m+[m[32m  -moz-hyphens: auto;[m
[32m+[m[32m  -webkit-hyphens: auto;[m
[32m+[m[32m  -ms-hyphens: auto;[m
[32m+[m[32m  hyphens: auto;[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m/* sets hyphenation by default on paragraphs */[m
[32m+[m[32mtextarea {[m
[32m+[m[32m  resize: vertical;[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m/* changes textarea resizing from "both" (UA default) to vertical only */[m
[32m+[m[32mtable {[m
[32m+[m[32m  border-collapse: collapse;[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mtd {[m
[32m+[m[32m  padding: 0.5rem;[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m/* causes tables to actually look like tables */[m
[32m+[m[32mimg {[m
[32m+[m[32m  border: none;[m
[32m+[m[32m  max-width: 100%;[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m/* removes outlines from linked images (for older browsers and IE), sets up images for responsive design layout */[m
[32m+[m[32minput[type=submit]::-moz-focus-inner,[m
[32m+[m[32minput[type=button]::-moz-focus-inner {[m
[32m+[m[32m  border: 0px;[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m/* removes the inner border effect from focused buttons for form elements in Firefox */[m
[32m+[m[32minput[type=search] {[m
[32m+[m[32m  -webkit-appearance: textfield;[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32minput[type=submit] {[m
[32m+[m[32m  -webkit-appearance: none;[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m/* removes the OS X appearance from HTML5 search inputs and submit buttons when viewed in Safari */[m
[32m+[m[32minput:required:after {[m
[32m+[m[32m  color: #f00;[m
[32m+[m[32m  content: " *";[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m/* sets up required form fields with the conventional following red asterix */[m
[32m+[m[32minput[type=email]:invalid {[m
[32m+[m[32m  background: #f00;[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m/* causes a visual alert in browsers that do client-side checking of HTML5 email field ‚Äì this may already be the default in some browsers. */[m
[32m+[m[32m.right {[m
[32m+[m[32m  float: right;[m
[32m+[m[32m  margin-left: 2rem;[m
[32m+[m[32m  clear: right;[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m.left {[m
[32m+[m[32m  float: left;[m
[32m+[m[32m  margin-right: 2rem;[m
[32m+[m[32m  clear: left;[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m/* allows a quick shortcut for floating elements left and right while avoiding the ‚Äúfloat quirk‚Äù problem */[m
[32m+[m[32msub, sup {[m
[32m+[m[32m  line-height: 0;[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m/* sets superscript and subscript text so that it doesn't throw off leading for elements in which they are included */[m
[32m+[m[32m#container {[m
[32m+[m[32m  background-color: blue;[m
[32m+[m[32m  margin: auto;[m
[32m+[m[32m  display: flex;[m
[32m+[m[32m  max-width: 960px;[m
[32m+[m[32m  width: 50%;[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m.row {[m
[32m+[m[32m  display: flex;[m
[32m+[m[32m  max-width: 960px;[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m.box {[m
[32m+[m[32m  text-align: center;[m
[32m+[m[32m  display: flex;[m
[32m+[m[32m  background-color: yellow;[m
[32m+[m[32m  width: 240px;[m
[32m+[m[32m}[m
[32m+[m
